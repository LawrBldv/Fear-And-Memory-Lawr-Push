    cameraShotID: Shack_VCloseUp
    animationTrigger: Talking 2
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - Shack_Dante
    speakerID: Shack_Dante
    dialogueText: Nothing makes sense... I just woke up, and I don't know where I
      am. I'm so lost...
    cameraShotID: ShackD_CloseUp
    animationTrigger: Sad
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - Shack_Veronica2
    speakerID: Shack_Veronica2
    dialogueText: "We all are. I don\u2019t even know how much time I\u2019ve spent
      here... I... don't even remember anything aside from my name."
    cameraShotID: Shack_VCloseUp
    animationTrigger: Talking 1
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - Shack_Dante
    speakerID: Shack_Dante
    dialogueText: ...
    cameraShotID: ShackD_CloseUp
    animationTrigger: HeadShake
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - Shack_Dante
    speakerID: Shack_Dante
    dialogueText: "Anyways, I\u2019m Dante. What\u2019s your name?"
