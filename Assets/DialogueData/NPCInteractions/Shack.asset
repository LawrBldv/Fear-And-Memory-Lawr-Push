    cameraShotID: ShackD_CloseUp
    animationTrigger: Wary
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - Shack_Veronica2
    speakerID: Shack_Veronica2
    dialogueText: No one's here, mister. It's just you, me, and the other ones that
