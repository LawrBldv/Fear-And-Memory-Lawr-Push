    animationTrigger: Talking
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - Shack_Veronica2
    speakerID: Shack_Veronica2
    dialogueText: Veronica
    cameraShotID: Shack_VCloseUp
    animationTrigger: Talking 1
    lineSFX: {fileID: 0}
    displayDuration: 3
  - participantsToSetVisible:
    - Shack_Dante
    speakerID: Shack_Dante
    dialogueText: "Wait, but what about the constabulary? It\u2019s already curfew,
